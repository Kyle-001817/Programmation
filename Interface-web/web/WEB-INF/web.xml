<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>Servelette.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Find</servlet-name>
        <servlet-class>Servelette.Find</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>Servelette.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TanyServlet</servlet-name>
        <servlet-class>Servelette.TanyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VersementServlet</servlet-name>
        <servlet-class>Servelette.VersementServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CarteServlet</servlet-name>
        <servlet-class>Servelette.CarteServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestServlet</servlet-name>
        <url-pattern>/RetraitServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Find</servlet-name>
        <url-pattern>/Find</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TanyServlet</servlet-name>
        <url-pattern>/TanyServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VersementServlet</servlet-name>
        <url-pattern>/VersementServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CarteServlet</servlet-name>
        <url-pattern>/CarteServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
